{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "strict": false,
    "jsx": "react-jsx",
    // "jsxImportSource": "react",
    "jsxImportSource": "preact",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  }
}
