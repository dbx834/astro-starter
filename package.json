{
  "type": "module",
  "scripts": {
    "start": "astro dev",
    "startc": "npm run devc",
    "dev": "astro dev",
    "devc": "astro dev --force",
    "dev-help": "astro dev --help",
    "build": "NODE_OPTIONS='--max-old-space-size=24576' astro build",
    "buildc": "npm run buildf",
    "buildf": "NODE_OPTIONS='--max-old-space-size=24576' astro build --force",
    "build-help": "astro build --help",
    "preview": "astro preview",
    "prepare": "husky",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "stylelint": "stylelint \"**/*.{css,scss,astro,html}\"",
    "stylelint:fix": "stylelint \"**/*.{css,scss,astro,html}\" --fix",
    "commitlint": "commitlint --from origin/main --to HEAD",
    "spell": "cspell --no-progress --cache --gitignore \"**/*\"",
    "spell:changed": "cspell --no-progress --cache --gitignore --since origin/main",
    "spell:md": "cspell --no-progress --cache --gitignore \"**/*.{md,mdx}\"",
    "deploy-preview": "netlify deploy",
    "deploy": "netlify deploy --prod"
  },
  "overrides": {
    "react": "npm:@preact/compat@latest",
    "react-dom": "npm:@preact/compat@latest"
  },
  "lint-staged-strict": {
    "*.{md,mdx,txt,astro,html,js,jsx,ts,tsx,css,scss,json,yml,yaml}": [
      "cspell --no-progress --no-summary --cache --gitignore --stdin --stdin-filename \"{filepath}\""
    ],
    "*.{js,jsx,ts,tsx,astro}": [
      "eslint --fix --cache --cache-location .eslintcache"
    ],
    "*.{css,scss,astro,html}": [
      "stylelint --fix --cache"
    ],
    "*.{astro,md,css,scss,html,json,yml,yaml}": [
      "prettier --write"
    ]
  },
  "lint-staged": {
    "*.{astro,md,css,scss,html,json,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@astrojs/netlify": "^6.6.4",
    "@astrojs/partytown": "^2.1.4",
    "@astrojs/preact": "^4.1.3",
    "@astrojs/react": "^4.4.2",
    "@astrojs/sitemap": "^3.6.1",
    "@nanostores/persistent": "^1.2.0",
    "@nanostores/react": "^1.0.0",
    "@radix-ui/react-slot": "^1.2.4",
    "@react-spring/web": "^10.0.3",
    "@supabase/supabase-js": "^2.90.1",
    "@swup/astro": "^1.7.0",
    "@tailwindcss/vite": "^4.1.18",
    "airtable": "^0.12.2",
    "astro": "^5.16.7",
    "astro-capo": "^0.0.1",
    "astro-compressor": "^1.2.0",
    "astro-critters": "^2.2.1",
    "astro-favicons": "^3.1.5",
    "astro-font": "^1.1.0",
    "astro-lenis": "^1.0.5",
    "astro-min": "^1.3.1",
    "astro-page-insight": "^0.10.2",
    "astro-purgecss": "^5.3.0",
    "astro-seo-meta": "^5.2.0",
    "astro-seo-schema": "^5.1.0",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "lenis": "^1.3.17",
    "less": "^4.5.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.562.0",
    "markdown-to-jsx": "7.7.13",
    "minimist": "^1.2.8",
    "nanostores": "^1.1.0",
    "onnxruntime-node": "^1.23.2",
    "preact": "^10.28.2",
    "purgecss": "^7.0.2",
    "react-player": "^3.4.0",
    "react-toggle-dark-mode": "^1.1.1",
    "schema-dts": "^1.1.5",
    "sharp": "^0.34.5",
    "smartcrop-sharp": "^2.0.8",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "video.js": "^8.23.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@eslint/js": "^9.39.2",
    "@kindspells/astro-shield": "^1.7.1",
    "@playform/inline": "^0.1.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@unocss/reset": "^66.5.12",
    "astro-devtool-breakpoints": "^0.3.0",
    "astro-html-beautifier": "^1.0.4",
    "astro-lighthouse": "^0.1.3",
    "astro-og": "^0.3.1",
    "astro-relative-links": "^0.4.2",
